<p id="notice"><%= notice %></p>

<h1>Ordenes De Compra</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Material</th>
      <th>Cantidad</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @orden_de_compras.each do |orden_de_compra| %>
      <tr>
        <td><%= Material.find_by(id: orden_de_compra.material_id).descripcion %></td>
        <td><%= orden_de_compra.cantidad %></td>
        <td><%= link_to 'Detalles', orden_de_compra %></td>
        <td><%= link_to 'Editar', edit_orden_de_compra_path(orden_de_compra) %></td>
        <td><%= link_to 'Cotizar', "#" %></td>
        <td><%= link_to 'Eliminar', orden_de_compra, method: :delete, data: { confirm: 'Â¿Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
